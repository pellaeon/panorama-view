{
	"manifest_version": 2,
	"name": "Panorama View",
	"description": "Tab Groups with Panorama View",
	"version": "0.6.20",
	"icons": {
		"16": "icons/logo/logo-16.png",
		"24": "icons/logo/logo-24.png",
		"32": "icons/logo/logo-32.png",
		"48": "icons/logo/logo-48.png",
		"64": "icons/logo/logo-64.png",
		"96": "icons/logo/logo-96.png"
	},
	"applications": {
		"gecko": {
			"id": "test@test.net"
		}
	},

	"background": {
		"scripts": ["js/background.js", "js/thumbnail.js", "js-ported/utils.js", "js-ported/background.js"]
	},

	"browser_action": {
		"default_icon": "icons/icon.svg",
		"default_popup": "popup/popup.html"
	},

	 "options_ui": {
		"page": "options.html"
	},

	"permissions": [
		"<all_urls>",
		"tabs",
		"tabHide",
		"storage",
		"sessions",
		"downloads"
	]
}
